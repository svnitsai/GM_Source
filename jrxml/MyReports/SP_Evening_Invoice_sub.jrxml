	<queryString language="SQL">
		<![CDATA[SELECT DC.PayCReferenceNumber, DC.CustCode, 
c.custname, 
DC.InvoiceAmount, 
DC.PayCStatus, DC.InvoiceReferenceNumber,  DCD.PayCDate, DCD.SupplierCode, 
DCD.PaidAmount
FROM DailyPayC DC 
join DailyPayCDetails DCD ON DC.PayCReferenceNumber = DCD.PayCReferenceNumber
join Customer c on c.custcode = dc.custcode and c.custtype = 'Merchant' 
join Customer s on s.custcode = dcd.suppliercode and s.custtype = 'Supplier' 
join CustomerBanks CB1 on CB1.CustBankId = DCD.SupplierBankId and s.custcode = cb1.custcode 
where dcd.suppliercode = 200000000]]>
	</queryString>
